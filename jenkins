pipeline {
    agent any 

    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/your-username/your-repo.git' 
            }
        }
        stage('Build') {
            steps {
                // Use your build tool command (e.g., Maven)
                sh 'mvn clean install' 
            }
        }
        stage('Test') {
            steps {
                // Run your tests 
                sh 'mvn test' 
            }
        }
    }
}
